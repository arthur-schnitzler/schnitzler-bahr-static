   <text type="diaryDay">
      <body>
         <div type="writingSession" n="1">
            <p>
               <date when="1897-11-15" n="01"/>
               <rs type="person" ref="#pmb2888">Wertheimer</rs> abschicken</p>
            <p>
               <rs type="person" ref="#pmb12858">Stößl</rs>, <rs type="person" ref="#pmb12832">Steiner</rs>, <rs type="person" ref="#pmb11036">Burckhard</rs>, <rs type="person" ref="#pmb3293">Waniek</rs>
            </p>
            <p>
               <rs type="person" ref="#pmb21867">Neuberger</rs>
            </p>
            <p>
               <rs type="person" ref="#pmb13082">Karlweis</rs>
               <space unit="chars" quantity="10"/>
               <rs type="person" ref="#pmb11036">Burckhard</rs>
            </p>
            <p>–</p>
            <p>
               <rs type="person" ref="#pmb3297">Lenoble</rs>
            </p>
            <p>
               <hi rend="underline" n="1">
                  <rs type="org" ref="#pmb29304">Reichswehr</rs>
               </hi>
            </p>
            <p>
               <hi rend="underline" n="1">
                  <rs type="person" ref="#pmb11524">Groß</rs>, <rs type="person" ref="#pmb2121">Schnitzler</rs>
               </hi>
            </p>
            <p>
               <table rows="2" cols="2">
                  <row>
                     <cell>
                        <rs type="person" ref="#pmb27662">Kugel</rs>
                     </cell>
                     <cell>1) <unclear>kein</unclear> Photogr</cell>
                  </row>
                  <row>
                     <cell/>
                     <cell>2) <anchor type="commentary" xml:id="KD401-1"/>Photogr. von Band<note type="commentary" xml:id="KD401-1h">
                           <p>Es dürfte sich um das den <rs type="work" ref="#pmb31275">Studien zur Kritik der
                  Moderne</rs> vorangestellte Foto handeln.</p>
                        </note>.</cell>
                  </row>
               </table>
            </p>
            <p>
               <rs type="person" ref="#pmb3300">Löwy</rs>
            </p>
            <p>–</p>
         </div>
      </body>
   </text>
