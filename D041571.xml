   <text type="diaryDay">
      <body>
         <div type="writingSession" n="1">
            <p rend="right">S. <date when="1918-09-15" n="1">15. 9. 18</date>
            </p>
            <p>Geld? 150</p>
            <p>
               <rs type="person" ref="#pmb18567">Horsetzky</rs> wegen <rs type="person" ref="#pmb2249">Korff</rs>
            </p>
            <p>
               <rs type="person" ref="#pmb23452">Rittner</rs> (das andere Stück)</p>
            <p>
               <rs type="person" ref="#pmb2121">Schnitzler</rs>, ob um drei daheim | Adresse</p>
            <p>
               <rs type="person" ref="#pmb2121">Schnitzler</rs>
               <space unit="chars" quantity="1"/>
               <anchor type="textConst" xml:id="TD571-1">
                  <rs type="place" ref="#pmb150">Sternwartestr 71</rs>.</anchor>
            </p>
            <p>Notiz 5 <rs type="person" subtype="implied" ref="#pmb16477">Frankfurter</rs> – <rs type="person" ref="#pmb4012">Smekal</rs>
            </p>
            <p>–</p>
            <p>1 Uhr <rs type="person" ref="#pmb12481">Redlich</rs>
            </p>
            <p>Brief oder Karte <rs type="person" ref="#pmb10818">Anna</rs>
            </p>
         </div>
      </body>
