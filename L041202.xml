         <div type="writingSession" n="1">
            <p>
               <rs type="work" ref="#pmb29983">Dem Meister</rs>.– 10<c rendition="#prozent"/>!</p>
            <p>
               <date>14. 12. 03</date>
            </p>
            <p>
               <anchor type="commentary" xml:id="KL202-1"/>
               <rs type="work" ref="#pmb29909">Unter sich</rs>
               <note type="commentary" xml:id="KL202-1h">
                  <p>Die <rs type="org" ref="#pmb29779">11 Scharfrichter</rs> hatten <rs type="person" ref="#pmb10815">Bahrs</rs> Schauspiel <rs type="work" ref="#pmb29909">Unter
                  sich</rs> am <date >9. 12. 1903</date> in <rs type="place" ref="#pmb50">Wien</rs>
               gegeben.</p>
               </note>:</p>
            <closer>
               <signed>Arthur</signed>
            </closer>
            <postscript>
               <p>
                  <handShift scribe="pmb2173"/>
                  <signed>Olga S.</signed>
                  <lb/>
                  <handShift scribe="pmb11740"/>
                  <signed>Hugo (ich bin nicht der <rs type="work" subtype="implied" ref="#pmb29909">
                        <anchor type="commentary" xml:id="KL202-2"/>Onkel<note type="commentary" xml:id="KL202-2h">
                           <p>Figur aus <rs type="work" ref="#pmb29909">Unter sich</rs>.</p>
                        </note>
                     </rs>)</signed>
                  <lb/>
                  <handShift scribe="pmb2292"/>
                  <signed>Gerty</signed>
                  <lb/>
                  <handShift scribe="pmb10863"/>
                  <signed>Richard</signed>
               </p>
            </postscript>
         </div>
      </body>
