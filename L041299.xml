   <text type="telegram">
      <body>
         <div type="writingSession" n="1">
            <p>
               <rs type="person" ref="#pmb10818">Mildenburg</rs>
               <space unit="chars" quantity="1"/>
               <anchor type="commentary" xml:id="KL299-1"/>singt morgen<note type="commentary" xml:id="KL299-1h">
                  <p>Am <date when="1906-03-24" >24. 3. 1906</date> brachte die <rs type="place" ref="#pmb15">Hofoper</rs>
                     <space unit="chars" quantity="1"/>
                     <rs type="work" ref="#pmb30613">Don Giovanni</rs>; <rs type="person" ref="#pmb10818">Mildenburg</rs> sang <rs type="work" subtype="implied" ref="#pmb30613">Donna
                  Anna</rs>.</p>
               </note> herzlichst <signed>hermann</signed>
            </p>
         </div>
      </body>
   </text>
